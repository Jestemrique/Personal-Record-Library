{
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "dev-personal-record-library.vercel.app"
        }
      ],
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://app.storyblok.com"
        }
      ]
    }
  ]
}